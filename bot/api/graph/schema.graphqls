# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  name: String!
}

type Query {
  users: [User!]!
}

input MigrateInput {
  version: String
}

type MigrateOutput {
  success: Boolean
  version: String
}

type Mutation {
  migrate(input: MigrateInput!): MigrateOutput
}
